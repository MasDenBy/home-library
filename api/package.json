{
  "name": "masden-home-library-api",
  "version": "0.0.1",
  "description": "",
  "author": "Ihar Maiseyeu",
  "private": true,
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/MasDenBy/home-library/issues"
  },
  "homepage": "https://github.com/MasDenBy/home-library#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MasDenBy/home-library.git"
  },
  "keywords": [
    "nodejs",
    "homelibrary",
    "synology",
    "ts",
    "typescript"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "./node_modules/.bin/forever start dist/main.js",
    "stop:prod": "./node_modules/.bin/forever stop dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "premigration": "(del ormconfig.json || :) && node dist/scripts/write-type-orm-config.js",
    "migration": "./node_modules/.bin/typeorm migration:run"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "^8.0.2",
    "app-root-path": "^3.0.0",
    "chokidar": "^3.5.2",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "forever": "^4.0.3",
    "mysql": "^2.18.1",
    "nanoid": "^3.1.31",
    "nest-winston": "^1.6.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.37",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/app-root-path": "^1.2.4",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.module.ts",
      "!**/migrations/*",
      "!**/scripts/*",
      "!main.ts",
      "!**/config.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
